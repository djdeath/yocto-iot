# LAYER_CONF_VERSION is increased each time build/conf/bblayers.conf
# changes incompatibly
LCONF_VERSION = "6"

BBPATH = "${TOPDIR}"
BBFILES ?= ""

BBLAYERS ?= " \
  ##OEROOT##/meta \
  ##OEROOT##/meta-oe \
  ##OEROOT##/meta-clanton-distro \
  ##OEROOT##/meta-yocto \
  ##OEROOT##/meta-intel \
  ##OEROOT##/meta-clanton-bsp \
  ##OEROOT##/meta-clanton-galileo \
  "
BBLAYERS_NON_REMOVABLE ?= " \
  ##OEROOT##/meta \
  ##OEROOT##/meta-yocto \
  "

