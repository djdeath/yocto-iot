+}
+
+/**
+ * intel_cln_gpio_irq_enable
+ * @param irq_data: Pointer to information about the IRQ
+ *
+ * Enable interrupts for a GPIO.
+ */
+static void intel_cln_gpio_irq_enable(struct irq_data *d)
+{
+	unsigned long flags = 0;
