+#define CLN_UART_DMA_BUF_SIZE PAGE_SIZE
+
+#define CLN_UART_MODE_MSI		0x00000001
+#define CLN_UART_MODE_DMA		0x00000002
+
+#define CLN_UART_DEFAULT_UARTCLK	1843200 /*   1.8432 MHz */
+
