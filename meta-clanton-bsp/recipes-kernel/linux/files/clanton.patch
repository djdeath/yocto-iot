+#define INTEL_CLN_UART_IIR_MS		0x00
+#define INTEL_CLN_UART_IIR_NOIRQ	0x01
+#define INTEL_CLN_UART_IIR_THRE		0x02
+#define INTEL_CLN_UART_IIR_RXD		0x04
+#define INTEL_CLN_UART_IIR_RLS		0x06
